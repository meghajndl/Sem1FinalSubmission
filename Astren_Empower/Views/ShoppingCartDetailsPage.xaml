<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="MDEV1014PracticeProject.Views.ContactDetailsPage" BackgroundColor="{StaticResource PageBackgroundColor}">
    <ContentPage.Content>
        <Grid>
            <Label Grid.Row="0" Grid.Column="0" Text="Name"/>
            <Label Grid.Row="0" Grid.Column="1" Text="{Binding cName}"/>

            <Label Grid.Row="1" Grid.Column="0" Text="Email"/>
            <Label Grid.Row="1" Grid.Column="1" Text="{Binding cEmail}"/>

            <Label Grid.Row="2" Grid.Column="0" Text="College"/>
            <Label Grid.Row="2" Grid.Column="1" Text="{Binding cCollege}"/>

        </Grid>
    </ContentPage.Content>
</ContentPage>
